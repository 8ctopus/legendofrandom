{
    "name": "octopuslabs/legend",
    "description": "Legend of Random site copy",
    "type": "project",
    "license": "MIT",
    "homepage": "https://legend.octopuslabs.io/",
    "require": {
        "php": "^8.3",
        "8ctopus/nano-router": "^14.0",
        "8ctopus/nano-timer": "^4.0",
        "httpsoft/http-emitter": "^1.1",
        "httpsoft/http-message": "^1.1",
        "httpsoft/http-server-request": "^1.1",
        "league/iso3166": "^4.3",
        "maxmind-db/reader": "^1.12",
        "twig/twig": "^3.6",
        "8ctopus/nano-ip": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-latest"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "8.3"
        }
    },
    "autoload": {
        "psr-4": {
            "Legend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-html=coverage"
    }
}
